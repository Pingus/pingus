<?xml version="1.0"  encoding="ISO-8859-1"?>

<pingus-level>
  <global>
    <levelname lang="en">EditorDefaultValue: Please give me a name...</levelname>
    <description lang="en">EditorDefaultValue: ...and a short description</description>
    <author>Ingo Ruhnke &lt;grumbel@gmx.de&gt;</author>
    <number-of-pingus>50</number-of-pingus>
    <number-to-save>25</number-to-save>
    <time>-1</time>
    <width>1216</width>
    <height>608</height>
    <difficulty>40</difficulty>
    <playable>0</playable>
    <comment></comment>
    <music>none</music>
  </global>

  <action-list>
    <basher count="20"/>
    <blocker count="20"/>
    <bomber count="20"/>
    <bridger count="20"/>
    <climber count="20"/>
    <digger count="20"/>
    <floater count="20"/>
    <jumper count="20"/>
    <miner count="20"/>
  </action-list>

  <group>
    <hotspot>
      <surface><resource type="datafile">
          <resource-datafile>hotspots</resource-datafile>
          <resource-ident>signposts/arrow_south</resource-ident>
          <modifier>ROT0</modifier>
        </resource></surface>
      <position>
        <x-pos>361</x-pos>
        <y-pos>287</y-pos>
        <z-pos>0</z-pos>
      </position>
      <speed>-1</speed>
      <parallax>1</parallax>
    </hotspot>

    <hotspot>
      <surface><resource type="datafile">
          <resource-datafile>hotspots</resource-datafile>
          <resource-ident>signposts/arrow_northwest</resource-ident>
          <modifier>ROT0</modifier>
        </resource></surface>
      <position>
        <x-pos>454</x-pos>
        <y-pos>301</y-pos>
        <z-pos>0</z-pos>
      </position>
      <speed>-1</speed>
      <parallax>1</parallax>
    </hotspot>

    <group>
      <worldobj type="groundpiece">
        <type>ground</type>
        <surface><resource type="datafile">
            <resource-datafile>groundpieces-ground</resource-datafile>
            <resource-ident>misc/bpiece8</resource-ident>
            <modifier>ROT0</modifier>
          </resource></surface>
        <position>
          <x-pos>494</x-pos>
          <y-pos>121</y-pos>
          <z-pos>0</z-pos>
        </position>
      </worldobj>

      <worldobj type="groundpiece">
        <type>ground</type>
        <surface><resource type="datafile">
            <resource-datafile>groundpieces-ground</resource-datafile>
            <resource-ident>misc/bpiece7</resource-ident>
            <modifier>ROT0</modifier>
          </resource></surface>
        <position>
          <x-pos>345</x-pos>
          <y-pos>112</y-pos>
          <z-pos>0</z-pos>
        </position>
      </worldobj>

    </group>

  </group>

  <start-position>
    <position>
      <x-pos>50</x-pos>
      <y-pos>50</y-pos>
      <z-pos>1000</z-pos>
    </position>
  </start-position>

</pingus-level>

